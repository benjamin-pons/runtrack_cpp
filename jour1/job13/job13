#include <iostream>

int main() {
    int somme = 0, nombre;

    for (int i = 1; i <= 5; ++i) {
        std::cout << "Entrez le " << i << "Ã¨me entier : ";
        std::cin >> nombre;
        somme += nombre;
    }

    std::cout << "La moyenne des cinq entiers est : " << somme / 5.0 << std::endl;

    return 0;
}